[alias]
  ca = commit --amend
  d = diff
  p = push
  pl = pull
  s = status
  sc = stash clear
  sl = stash list
  sp = stash pop
[color "branch"]
  local = blue
  remote = yellow
[color "diff"]
  func = cyan
  meta = yellow
  whitespace = red
[color "status"]
  untracked = yellow
[core]
  editor = vim
  excludesfile = ~/.gitignore
[credential]
  helper = osxkeychain
[push]
  default = upstream
[user]
  email = iPetesta@gmail.com
  name = Pete Cruz
